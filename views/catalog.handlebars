<div class="container">
        <h2 class="gametext center">Catalog</h2>

        <div class="row">
            <form class="col s12">
                <div class="row">
                    <div class="input-field col s3">
                        <select>
                            <option value="" disabled selected></option>
                            <option id="Game">Game</option>
                            <option id="User">User</option>
                        </select>
                        <label class="black-text">User or Game</label>
                    </div>
                    <div class="input-field col s3">
                        <select>
                            <option value="" disabled selected></option>
                            <option id="Owned">Owned</option>
                            <option id="Requested">Requested</option>
                        </select>
                        <label class="black-text">Owned or Requested</label>
                    </div>
                    <div class="input-field col s6">
                        <input id="search" type="text" class="validate">
                        <label for="search">Search Term</label>
                    </div>
                </div>
                <div class="row">
                    <button class="waves-effect waves-red btn-flat white">Search</button>
                </div>
            </form>
        </div>
        <div class="row">
            <div class="col s12">
                <p class="gametext">Look what we got!</p>
                <table>
                    <thead>
                        <tr>
                            <th>Game</th>
                            <th># of players</th>
                            <th>Genre</th>
                            <th>User</th>
                        </tr>
                    </thead>
                    <tbody>
                    {{#each game}}
                        <tr>
                            <td>{{this.name}}</td>
                            <td>{{this.player}}</td>
                            <td>{{this.genre}}</td>
                            <td>{{this.user}}</td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <script>
        $(document).ready(function () {
            $('select').formSelect();
        });
    </script>